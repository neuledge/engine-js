{
  "name": "@neuledge/scalars",
  "version": "0.1.0-alpha.0",
  "description": "Scalar definitions for Neuledge Engine",
  "keywords": [
    "neuledge",
    "engine",
    "store",
    "scalars",
    "integer",
    "float",
    "string",
    "boolean",
    "datetime",
    "buffer",
    "bigint",
    "number",
    "object"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neuledge/engine-js.git"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "types": "rimraf dist/**/*.d.ts dist/**/*.d.ts.map && tsc --emitDeclarationOnly",
    "build": "rimraf dist/**/*.js dist/**/*.js.map && tsup",
    "test": "jest",
    "lint": "eslint . --ext \"js,jsx,ts,tsx,mjs,cjs\"",
    "lint:strict": "yarn lint --max-warnings 0"
  },
  "dependencies": {
    "@neuledge/store": "^0.1.0-alpha.0",
    "fast-deep-equal": "^3.1.3",
    "zod": "^3.20.6"
  }
}
