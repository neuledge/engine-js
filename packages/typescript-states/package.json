{
  "name": "@neuledge/typescript-states",
  "version": "0.1.0",
  "description": "State code generator for Neuledge Engine in TypeScript",
  "keywords": [
    "typescript",
    "state",
    "code",
    "generator",
    "neuledge",
    "engine"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neuledge/engine-js.git"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "types": "rimraf dist/**/*.d.ts dist/**/*.d.ts.map && tsc --emitDeclarationOnly",
    "build": "rimraf dist/**/*.js dist/**/*.js.map && tsup",
    "test": "jest",
    "lint": "eslint . --ext \"js,jsx,ts,tsx,mjs,cjs\"",
    "lint:strict": "yarn lint --max-warnings 0"
  },
  "dependencies": {
    "@neuledge/states": "^0.1.0"
  }
}
