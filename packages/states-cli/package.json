{
  "name": "@neuledge/states-cli",
  "version": "0.1.1",
  "description": "Neuledge Engine states CLI for parsing and converting *.states files",
  "keywords": [
    "neuledge",
    "engine",
    "states",
    "cli",
    "typescript",
    "javascript",
    "parser"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "states": "bin/states.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neuledge/engine-js.git"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "types": "rimraf dist/**/*.d.ts dist/**/*.d.ts.map && tsc --emitDeclarationOnly",
    "build": "rimraf dist/**/*.js dist/**/*.js.map && tsup",
    "test": "jest",
    "lint": "eslint . --ext \"js,jsx,ts,tsx,mjs,cjs\"",
    "lint:strict": "yarn lint --max-warnings 0"
  },
  "dependencies": {
    "@neuledge/states": "^0.1.3",
    "@neuledge/typescript-states": "^0.1.3",
    "commander": "^10.0.0",
    "fast-glob": "^3.2.12",
    "p-limit": "^3.1.0"
  }
}
