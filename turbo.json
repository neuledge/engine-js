{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": [],
      "inputs": ["src/**"]
    },
    "lint": {
      "outputs": []
    },
    "lint:strict": {
      "outputs": []
    },
    "deploy": {
      "dependsOn": ["build", "test", "lint:strict"],
      "outputs": []
    }
  }
}
